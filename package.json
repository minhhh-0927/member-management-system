{
  "name": "member-management-system",
  "version": "0.0.0",
  "description": "Member Management System",
  "author": "Sun* Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "migrate": "typeorm migration:run",
    "start": "NODE_ENV=development ts-node --type-check -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^6.8.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^6.8.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^6.2.0",
    "@types/passport-jwt": "^3.0.3",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.10.1",
    "dotenv": "^8.2.0",
    "helmet": "^4.1.0",
    "js-yaml": "^3.12.0",
    "mysql2": "^1.6.4",
    "nconf": "^0.10.0",
    "nunjucks": "^3.2.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.11.4",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.7.11",
    "@types/nunjucks": "^3.1.3",
    "@types/supertest": "^2.0.5",
    "jest": "^24.5.0",
    "nodemon": "^1.18.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.12.0",
    "tslint-config-5minds": "^1.0.6",
    "typescript": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
